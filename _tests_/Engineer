const Engineer = require("../lib/Engineer");

test("Can create a github.", () => {
    const testGithub = "lib1001";
    const newEmployee = new Engineer("Lauren", 2, "lburke522@gmail.com", testGithub);
    expect(newEmployee.github).toBe(testGithub);
});

test("Testing getGithub will return github.", () => {
    const testGithub = "lib1001";
    const newEmployee = new Engineer("Lauren", 2, "lburke522@gmail.com", testGithub);
    expect(newEmployee.getGithub()).toBe(testGithub);
});

test("Testing role.", () => {
    const returnValue = "Engineer";
    const newEmployee = new Engineer("Lauren", 2, "lburke522@gmail.com", "lib1001");
    expect(newEmployee.getRole()).toBe(returnValue);
});